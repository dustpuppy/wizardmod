package thewizardmod.fluids;

import net.minecraft.client.gui.inventory.GuiContainer;
import net.minecraft.inventory.Container;
import net.minecraft.util.ResourceLocation;

public class GuiTank extends GuiContainer{

	public static final ResourceLocation BACKGROUND = new ResourceLocation("thewizardmod:textures/gui/tank_bg.png");
	
	public GuiTank(Container container) {
		super(container);
	}

	@Override
	protected void drawGuiContainerBackgroundLayer(float partialTicks, int mouseX, int mouseY) {
		mc.renderEngine.bindTexture(BACKGROUND);
	}

}
